{
  "name": "MyStylingApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "extract-intl": "babel-node --presets env,stage-0 -- ./internals/scripts/extract-intl.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test:clean": "rimraf ./coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "pretest": "npm run test:clean && npm run lint",
    "test": "jest",
    "build": "yarn test __tests__/build.test.js && react-native run-android --variant=release",
    "generate": "kerosene",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prettify": "prettier --write"
  },
  "rnpm": {
    "assets": [
      "./app/assets"
    ]
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "intl": "^1.2.5",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.5",
    "prop-types": "^15.5.10",
    "react": "16.8.3",
    "react-intl": "^2.8.0",
    "react-native": "0.59.1",
    "react-native-animatable": "^1.3.1",
    "react-native-fritz": "https://github.com/zsajjad/react-native-fritz",
    "react-native-gesture-handler": "~1.0.14",
    "react-native-image-picker": "^0.28.0",
    "react-native-iphone-x-helper": "^1.2.0",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-permissions": "^1.1.1",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-native-vector-icons": "^6.3.0",
    "react-navigation": "^3.3.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "warning": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-react-intl": "3.0.1",
    "coveralls": "3.0.3",
    "create-react-class": "^15.6.2",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-import-resolver-babel-module": "5.0.1",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-native": "^3.1.0",
    "eslint-plugin-redux-saga": "1.0.0",
    "jest": "24.1.0",
    "jest-cli": "24.1.0",
    "kerosene": "../../kerosene/",
    "lint-staged": "8.1.5",
    "metro-react-native-babel-preset": "^0.53.1",
    "mocha": "^6.0.2",
    "pre-commit": "1.2.2",
    "prettier": "^1.14.2",
    "react-test-context-provider": "git+https://github.com/zsajjad/react-test-context-provider.git",
    "react-test-renderer": "16.8.3",
    "rimraf": "2.6.3",
    "serial-mocha": "^0.0.4",
    "shelljs": "0.8.3",
    "sinon": "7.2.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
